<%- include('partials/header') %>


  <% pokemons.forEach( (pokemon) => {%>
            
    <div class="pokeCard">
        <a class="pokeCard__a" href="/pokemonDetail/<%= pokemon.id %>">
            <img class="pokeCard__img" src="/img/<%= pokemon.numero %>.png" alt="<%= pokemon.nom %> illustration">
            <p class="pokeCard__name">#<%= pokemon.numero %> <%= pokemon.nom %></p>
        </a>
    </div>
<% }) %>
</div>


<%- include('partials/footer') %>